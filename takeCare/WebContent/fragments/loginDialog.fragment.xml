<Dialog xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:layout="sap.ui.layout"
	title="Login">
	<layout:VerticalLayout width="100%">
		<layout:content>
				<Input type="Email" id="userName" placeholder="Enter Email ID"></Input>
				<Input type="Password" id="password" placeholder="Enter Password"></Input>
			</layout:content>
	</layout:VerticalLayout>
	<beginButton>
		<Button text="Login" press="onPressLogin"></Button>
	</beginButton>
	<endButton>
		<Button text="Cancel" press="onPressHandleClose"></Button>
	</endButton>
</Dialog>