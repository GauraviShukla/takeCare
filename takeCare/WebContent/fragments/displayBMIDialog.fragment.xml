<Dialog xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:layout="sap.ui.layout" title="Your BMI">
	<layout:VerticalLayout width="100%">
		<layout:content>
			<Text id="bmiValue"></Text>
			<Text text="underweight (BMI less than 18.5)"></Text>
			<Text text="Normal Weight (BMI between 18.5 and 24.9)"></Text>
			<Text text="Over Weight (BMI between 25 and 29.9)"></Text>
			<Text text="obese (BMI 30.0 and above)"></Text>
			<Text id="weightType"></Text>
		</layout:content>
	</layout:VerticalLayout>
	<endButton>
		<Button text="Cancel" press="onPressHandleClose"></Button>
	</endButton>
</Dialog>