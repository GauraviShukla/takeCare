<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="takecare.searchResults" xmlns:html="http://www.w3.org/1999/xhtml">
			<TileContainer id="container" height="600px" width="1000px" tiles="{/Hospitals}">
				<CustomTile xmlns="sap.m" press="" class="tile" >
					<tooltip></tooltip> <!-- sap.ui.core.TooltipBase -->
					<content>
						<VBox xmlns="sap.m" class="tileVBox" >
							<items>
								<HBox xmlns="sap.m" class="titleNameBox" >
									<items>
										<Image xmlns="sap.m" src="{image}" width="" height=""></Image>
										<Text xmlns="sap.m" text="{name}" class="nameFiled" ></Text>
									</items>
								</HBox>
								<Text xmlns="sap.m" text="{address}"></Text>
								<Text xmlns="sap.m" text="{contact}"></Text>
								<Text xmlns="sap.m" text="{ambulance}"></Text>
								<RatingIndicator xmlns="sap.m" iconSize="10px" value= "{rating}">	</RatingIndicator>
							</items>
						</VBox>
					</content> 
				</CustomTile>
			</TileContainer>
</core:View>