<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="takecare.searchResults" xmlns:html="http://www.w3.org/1999/xhtml">
	<Grid xmlns="sap.ui.layout" id="container" 	width="100%" content="{/}" vSpacing="1.5"
	hSpacing="1.5" class="gridLayout">
 	 <CustomTile xmlns="sap.m"
			press="tilepress">
		<content>  
		<VBox xmlns="sap.m" class="tile" >
					<items>
						<HBox xmlns="sap.m" >
									<items>
										<Image xmlns="sap.m" src="{image}" width="" height=""></Image>
										 <VBox xmlns="sap.m" class="nameFieldVBox" >
											<items>
												<Text xmlns="sap.m" text="{name}" class="nameField"></Text>
												<Text xmlns="sap.m" text="{type}"></Text>
												<Text xmlns="sap.m" text="{address}"></Text>
			   									<Text xmlns="sap.m" text="{contact}"></Text>
												<Text xmlns="sap.m" text="{ambulance}"></Text>
											</items>
										</VBox>
									</items>
							</HBox>
							<RatingIndicator class="rating" xmlns="sap.m" iconSize="10px" enabled="false" value= "{data>rating}">	</RatingIndicator>
						</items>
				</VBox>
 </content> 
</CustomTile> 

	</Grid>
</core:View>